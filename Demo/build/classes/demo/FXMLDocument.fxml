<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="200" prefWidth="320" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/17" fx:controller="demo.FXMLDocumentController">
    <children>
      <VBox layoutX="8.0" layoutY="9.0" prefHeight="191.0" prefWidth="312.0" spacing="10.0">
         <children>
            <Label text="Subject" />
            <CheckBox fx:id="phy" mnemonicParsing="false" text="Physics" />
            <CheckBox fx:id="chem" mnemonicParsing="false" text="Chemistry" />
            <CheckBox fx:id="bio" mnemonicParsing="false" text="Biology" />
              <Button fx:id="button" onAction="#whenClicked" text="Take" />
            <Label fx:id="label" wrapText="true" />
         </children>
      </VBox>
    </children>
</AnchorPane>
