<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="418.0" prefWidth="475.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/17" fx:controller="signuppage.FXMLDocumentController">
    <children>
        <Button fx:id="button" layoutX="309.0" layoutY="187.0" onAction="#registerClicked" prefHeight="44.0" prefWidth="98.0" text="Register!">
         <font>
            <Font size="18.0" />
         </font></Button>
        <Label fx:id="label" layoutX="126" layoutY="120" minHeight="16" minWidth="69" />
      <VBox layoutX="26.0" layoutY="47.0" prefHeight="342.0" prefWidth="190.0" spacing="10.0">
         <children>
            <Label text="First Name:" />
            <TextField fx:id="firstName" />
            <Label text="Last Name:" />
            <TextField fx:id="lastName" />
            <Label text="Email ID:" />
            <TextField fx:id="emailId" />
            <Label text="Username:" />
            <TextField fx:id="userName" />
            <Label text="Password:" />
            <PasswordField fx:id="password" prefHeight="26.0" prefWidth="190.0" />
         </children>
      </VBox>
      <Label layoutX="196.0" prefHeight="44.0" prefWidth="84.0" text="Sign Up" textFill="RED">
         <font>
            <Font size="23.0" />
         </font>
      </Label>
      <Label fx:id="error" layoutX="267.0" layoutY="232.0" />
    </children>
</AnchorPane>
